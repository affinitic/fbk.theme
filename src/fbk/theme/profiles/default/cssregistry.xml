<?xml version="1.0"?>
<object name="portal_css">

 <stylesheet title="fbk.theme styles"
    id="++theme++fbk.theme/css/theme-style.css"
    media="screen" rel="stylesheet" rendering="import"
    cacheable="True" compression="safe" cookable="True"
    enabled="1"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'fbk.theme'"
    applyPrefix="True"
    insert-after="++theme++fbk.theme/css/styles.css" />

 <stylesheet title="fbk.theme fonts"
    id="++theme++fbk.theme/css/fontello.css"
    media="screen" rel="stylesheet" rendering="import"
    cacheable="False" compression="safe" cookable="False"
    enabled="1"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'fbk.theme'"
    applyPrefix="True"
    insert-after="++theme++fbk.theme/css/style.css" />

</object>
